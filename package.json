{
  "name": "node-mongodb-template",
  "version": "1.0.0",
  "description": "A starter project for Node application written in Express framework with MongoDB accessible via mongoose",
  "main": "server.js",
  "keywords": [
    "Node.js",
    "Node",
    "Express.js",
    "express",
    "template",
    "seed",
    "MongoDB",
    "mongoose",
    "heroku",
    "swagger",
    "winston"
  ],
  "homepage": "https://github.com/pamigomp/node-mongodb-template",
  "bugs": {
    "url": "https://github.com/pamigomp/node-mongodb-template/issues"
  },
  "author": "Michal Pietrzak",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pamigomp/node-mongodb-template.git"
  },
  "scripts": {
    "postinstall": "if-env NODE_ENV=developemnt && git config core.hooksPath hooks || exit 0",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "cluster": "node cluster.js",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/unit/*.test.js",
    "test-integration": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/integration/*.test.js"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "consign": "0.1.6",
    "cookie-parser": "1.4.3",
    "cors": "2.8.5",
    "cross-env": "5.2.0",
    "dotenv": "6.2.0",
    "envalid": "4.1.4",
    "errorhandler": "1.5.0",
    "express": "4.16.4",
    "express-validator": "5.3.1",
    "helmet": "3.15.0",
    "if-env": "1.0.4",
    "jsonwebtoken": "8.4.0",
    "memory-cache": "0.2.0",
    "method-override": "3.0.0",
    "mongoose": "5.4.7",
    "mongoose-paginate": "5.0.3",
    "morgan": "1.9.1",
    "passport": "0.4.0",
    "passport-facebook": "3.0.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "response-time": "2.3.2",
    "swagger-jsdoc": "3.2.6",
    "swagger-ui-express": "4.0.2",
    "validator": "10.11.0",
    "winston": "3.2.0"
  },
  "devDependencies": {
    "eslint": "5.12.1",
    "eslint-plugin-node": "8.0.1",
    "mocha": "5.2.0",
    "nodemon": "1.18.9"
  },
  "engines": {
    "node": "10.3.0",
    "npm": "6.1.0"
  },
  "private": false
}
